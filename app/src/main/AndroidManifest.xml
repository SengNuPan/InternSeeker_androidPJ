<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sengnupan.android_pj">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/intern_seeker"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"

         />
        <activity
            android:name=".searchCompany"
            android:label="General Information"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".their_stories"
            android:label="About Companies"
            android:parentActivityName=".searchCompany" />
        <activity
            android:name=".their_activites"
            android:label="Albums"
            android:parentActivityName=".searchCompany" />
        <activity android:name=".Files" />
        <activity android:name=".webview" />
        <activity
            android:name=".about_us"
            android:label="About Intern Seeker"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Invite_Friends_Activity" />
        <activity android:name=".Share_Activity" />
        <activity android:name=".Feedback" />

        <activity android:name=".Favourite_Activity"
            android:label="Favorite List"
            android:parentActivityName=".MainActivity"  />

    </application>

</manifest>